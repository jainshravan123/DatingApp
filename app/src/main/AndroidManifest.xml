<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="myapp.dating.shravan.datingapp1">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.Trial_1" />
        <activity android:name=".activity.Trial_2" />
        <activity
            android:name=".activity.Signin"
            android:noHistory="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SignUp"
            android:noHistory="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SigningUp"
            android:noHistory="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.Splash"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.Dashboard"
            android:theme="@style/CustomStyle1" />
        <activity
            android:name=".activity.Question"
            android:theme="@style/CustomStyle1" />
        <activity
            android:name=".activity.nav_draw_activity.Profile"
            android:theme="@style/CustomStyle1" />
        <activity
            android:name=".activity.nav_draw_activity.Notification"
            android:theme="@style/CustomStyle1" />
        <activity
            android:name=".activity.nav_draw_activity.SearchPeople"
            android:theme="@style/CustomStyle1" />
        <activity
            android:name=".activity.nav_draw_activity.PersonalTagFilters"
            android:theme="@style/CustomStyle1" />
        <activity
            android:name=".activity.nav_draw_activity.Messages"
            android:theme="@style/CustomStyle1" />
        <activity
            android:name=".activity.nav_draw_activity.NewsFeed"
            android:theme="@style/CustomStyle1" />
        <activity
            android:name=".activity.nav_draw_activity.Gifts"
            android:theme="@style/CustomStyle1" />
        <activity
            android:name=".activity.nav_draw_activity.OnDemandDating"
            android:theme="@style/CustomStyle1" />
        <activity
            android:name=".activity.nav_draw_activity.DiscoverMode"
            android:theme="@style/CustomStyle1" />
        <activity
            android:name=".activity.nav_draw_activity.Upgrade"
            android:theme="@style/CustomStyle1" />
        <activity
            android:name=".activity.nav_draw_activity.Setting"
            android:theme="@style/CustomStyle1" />
        <activity
            android:name=".activity.UserDashboardProfile"
            android:theme="@style/CustomStyle1"></activity>

        <service
            android:name=".service.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>


        <service
            android:name=".service.MyFirebaseInstantIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>


    </application>

</manifest>
